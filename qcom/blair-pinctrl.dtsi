&soc {
	tlmm: pinctrl@400000 {
		compatible = "qcom,blair-pinctrl";
		reg = <0x400000 0x800000>;
		interrupts = <GIC_SPI 227 IRQ_TYPE_LEVEL_HIGH>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
		qcom,gpios-reserved = <13 14 15 16 17 45 46 48 56 57>;
		wakeup-parent = <&mpm>;
	};
};
